{
  "name": "@bond-london/simple-gatsby-source-graphcms",
  "version": "1.2.0",
  "description": "Simple version of the official Gatsby source plugin for GraphCMS projects",
  "main": "index.js",
  "scripts": {
    "build": "babel src --out-dir ./ --source-maps --extensions .ts",
    "watch": "babel --watch src --out-dir ./ --source-maps --extensions .ts"
  },
  "keywords": [
    "data",
    "gatsby",
    "gatsby-plugin",
    "graphcms",
    "graphql",
    "source"
  ],
  "license": "MIT",
  "dependencies": {
    "gatsby-graphql-source-toolkit": "^2.0.1",
    "gatsby-plugin-image": "^1.5.0",
    "gatsby-source-filesystem": "^3.5.0",
    "he": "1.2.0",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "babel-preset-gatsby-package": "^1.5.0",
    "gatsby": "^3.5.0"
  },
  "peerDependencies": {}
}
